{
  "nodes": {
    "user_id": "user",
    "product_id": "product",
    "content_name": "content",
    "tag": "interest",
    "genre": "interest"
  },
  "edges": [
    {
      "from": "user_id",
      "to": "product_id",
      "via": "orders",
      "relation": "purchased"
    },
    {
      "from": "user_id",
      "to": "content_name",
      "via": "streaming",
      "relation": "watched"
    },
    {
      "from": "product_id",
      "to": "tag",
      "via": "products",
      "relation": "tagged_as"
    },
    {
      "from": "content_name",
      "to": "genre",
      "via": "streaming",
      "relation": "about"
    }
  ]
}
